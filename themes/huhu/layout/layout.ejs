<!DOCTYPE html>
<html>
  <head>
    <%- gtm() %> <%- google_adsense() %> <%- baidu_tongji() %> <%- verification() %>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1" />
    <meta name="keywords" content="<%= config.keywords %>" />
    <meta name="description" content="<%= config.description%>" />
    <% let title = page.title; if (is_archive()) { title = __('archives') } if (is_month()) { title
    += page.year + '/' + page.month; } else if (is_year()) { title += page.year; } else if
    (is_current('categories')) { title = __('categories'); } else if (is_current('tags')) { title =
    __('tags'); } %>
    <title>
      <% if (title) { %><%= title %><% } else { %> <%= config.subtitle %><% }%> - <%= config.title
      %>
    </title>
    <link rel="manifest" href="/manifest.json" />
    <link rel="shortcut icon" href="<%= theme.favicon %>" type="image/x-icon" />
    <%- css('style/style') %>
  </head>
  <body>
    <div id="fixed-menu">
      <span class="iconfont icon-arrowup"></span>
    </div>
    <div id="progress">
      <div class="line"></div>
    </div>
    <div id="header">
      <div class="intro">
        <a href="/" class="logo" style="background-image: url('/images/logo.jpeg')"></a>
        <div class="author"><%= theme.author%></div>
      </div>
      <div class="nav">
        <span class="iconfont icon-menu toggle-icon"></span>
        <a href="javascript:void(0)" class="search-box">
          <span class="iconfont icon-sousuo"></span>
        </a>
      </div>
    </div>
    <div id="side" class="bounceInLeft">
      <div class="shrink">
        <a href="/" class="logo" style="background-image: url('/images/logo.jpeg')"></a>
        <span class="iconfont icon-menu toggle-icon"></span>
        <a href="javascript:void(0)" class="search-box">
          <span class="iconfont icon-sousuo"></span>
        </a>
      </div>
      <div class="magnify">
        <div class="about">
          <div class="author"><%= theme.author%></div>
          <a href="/" class="logo" style="background-image: url('/images/logo.jpeg')"></a>
        </div>

        <div class="nav">
          <% for (var i in theme.menu){ %>
          <a
            href="<%- url_for(theme.menu[i]) %>"
            class="<%= is_current(theme.menu[i]) ? 'current' : '' %>"
          >
            <%= __(i) %>
          </a>
          <% } %>
          <a href="javascript:void(0)" class="search-box">
            <span class="iconfont icon-sousuo"></span>
          </a>
        </div>
        <div class="bottom">
          <div class="follow">
            <% for (var f in theme.follow) { %>
            <a href="<%= theme.follow[f] %>" target="_block">
              <span class="iconfont icon-<%= f %>"></span>
            </a>
            <% } %> <% if (theme.rss) { %>
            <a href="<%= theme.rss %>" target="_block">
              <span class="iconfont icon-rss"></span>
            </a>
            <% } %>
          </div>
        </div>
      </div>
    </div>
    <div id="container">
      <div class="main">
        <%- body %>
        <footer>
          <div class="copyright">
            Â©<%= moment().format('YYYY') %>
            <a href="<%= config.url %>"><%= config.author %></a> Powered by
            <a href="https://hexo.io">Hexo</a> |
            <a href="https://github.com/shixiaohu2206/hexo-theme-huhu">hexo-theme-huhu</a>
          </div>
          <% if (theme.site_icp) { %>
          <div class="icp">
            <%= theme.site_icp %>
          </div>
          <% } %>
        </footer>
      </div>
    </div>
    <%- swiftype()%>
  </body>
  <%- gtm_noscript() %>
</html>
<%- huhu_config() %> <%- init_app() %> <%- baidu_push()%> <%- changyan()%>
